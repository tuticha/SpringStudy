<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="dbTest">

	<select id="getNumber" resultType="int">
		select 1 from dual
	</select>
	
	<select id="getString" resultType="String">
		select 'a' from dual
	</select>
	
	<select id="getList" resultType="int">
		select no from testDBcha
	</select>
	
	<select id="sumNumbers" resultType="int" parameterType="int">
		select 1 + #{number} from dual
	</select>
	
</mapper>